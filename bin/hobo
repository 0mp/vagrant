#!/usr/bin/env ruby
begin
  require File.expand_path('../.bundle/environment', __FILE__)
rescue LoadError
  # Fallback on doing the resolve at runtime.
  require "rubygems"
  require "bundler"
  Bundler.setup
end

require 'git-style-binary/command'

GitStyleBinary.primary do
  version "Somewhere between 0 and 0.1"

  run { educate }
end